{
  "name": "@repo/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "seed": "pnpm with-env tsx scripts/seed.ts",
    "seed-remote": "dotenv -e ../../.env.production.local -- tsx scripts/seed.ts",
    "with-env": "dotenv -e ../../.env --",
    "backfill-vectors": "pnpm with-env tsx scripts/backfill-vectors.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.27",
    "@better-auth/expo": "^1.4.18",
    "@repo/db": "workspace:^",
    "@trpc/server": "catalog:",
    "ai": "^6.0.82",
    "better-auth": "^1.4.18",
    "drizzle-zod": "^0.8.3",
    "next": "catalog:",
    "react": "catalog:",
    "superjson": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@kyh/eslint-config": "catalog:",
    "@kyh/prettier-config": "catalog:",
    "@kyh/tsconfig": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@kyh/prettier-config"
}
